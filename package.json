{
  "name": "framefusion",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "docs": "typedoc framefusion.ts",
    "tsc": "tsc",
    "dev": "vite",
    "build": "tsc && vite build && tsc --declaration",
    "prepare": "yarn run build",
    "preview": "vite preview",
    "test-once": "vitest run",
    "test": "vitest",
    "test:debug": "vitest --inspect-brk --single-thread",
    "test-ui": "vitest --ui",
    "vite-node": "vite-node",
    "debug": " yarn run build && node dist/framefusion.es.js",
    "debug:debug": " yarn run build && node --enable-source-maps --inspect-brk dist/framefusion.es.js"
  },
  "dependencies": {
    "beamcoder": "^0.7.1",
    "fs-extra": "^11.1.1",
    "got": "^12.6.0",
    "http-server": "^14.1.1",
    "tmp": "^0.2.1",
    "tsconfig-paths": "^4.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/http-server": "^0.12.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "@types/sinon": "^10.0.13",
    "@vitest/ui": "^0.29.8",
    "express": "^4.18.2",
    "jest-image-snapshot": "^6.1.0",
    "rimraf": "^4.4.1",
    "sinon": "^15.0.3",
    "typedoc": "^0.24.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-node": "^0.29.2",
    "vitest": "^0.29.2"
  },
  "description": "Video library",
  "main": "./dist/framefusion.es.js",
  "types": "./dist/framefusion.d.ts",
  "module": "./dist/framefusion.es.js",
  "exports": {
    ".": {
      "types": "./dist/framefusion.d.ts",
      "import": "./dist/framefusion.es.js"
    }
  },
  "files": [
    "dist/*"
  ]
}
